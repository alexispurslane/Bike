class Animal {
  def sound = "(...silence...)"
  def dangerous = false
  def wild = true
  def health = 100
  def attack_points = 25
  def init (sound1, dangerous1, wild1, health1, attack_points1) {
    println(attack_points1)
    self.sound = -> sound1 or self.sound
    self.dangerous = -> dangerous1 or self.dangerous
    self.wild = -> wild1 or self.wild
    self.health = -> health1 or self.health
    self.attack_points = -> attack_points1 or self.attack_points
  }
  def attack (other) {
    println(self.sound)
    Animal.new(other.sound, other.dangerous, other.wild, other.health - self.attack_points, other.attack_points)
  }
}
class Dog extends Animal {
  def sound = "Bark"
  def wild = false
  def attack_points = 20
}